# OpenAI API Key
OPENAI_API_KEY=your_api_key_here

# Optional: Custom Google API Key for better search results
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_custom_search_engine_id_here

# Configuration
MAX_IMAGES=100
MIN_IMAGE_WIDTH=800
MIN_IMAGE_HEIGHT=600
OUTPUT_DIR=./data/images

# GitHub API Configuration
# Required - Create a token at https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# Scraper Configuration
# Optional - Maximum number of images to scrape per page (default: 10)
MAX_IMAGES_PER_PAGE=10

# Optional - User agent to use for HTTP requests (default: scrape-images-for-issues)
USER_AGENT=scrape-images-for-issues

# Application Configuration
# Optional - Logging level (default: INFO)
# Available values: ERROR, WARN, INFO, DEBUG
LOG_LEVEL=INFO 